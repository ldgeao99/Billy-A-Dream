<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-config.dtd"> 
<configuration>
	<typeAliases>
		<typeAlias type="member.model.MemberBean" alias="MyMember"/>
		<typeAlias type="member.model.MemberIpBean" alias="MyMemberIp"/>
		<typeAlias type="event.model.EventBean" alias="MyEvent"/>
	</typeAliases>
	
	<mappers><!-- 하나만 있을때도 쓸 수 있다. -->
		<mapper resource="mybatis/lcategory.xml"/>
		<mapper resource="mybatis/scategory.xml"/>
		<mapper resource="mybatis/member.xml"/>
		<mapper resource="mybatis/memberIp.xml"/>
    	<mapper resource="mybatis/product.xml"/>
    	<mapper resource="mybatis/coupon.xml"/>
    	<mapper resource="mybatis/wishlist.xml"/>
    	<mapper resource="mybatis/reservation.xml"/>
    	<mapper resource="mybatis/chat.xml"/>
    	<mapper resource="mybatis/room.xml"/>

    	<mapper resource="mybatis/contect.xml"/>
    	<mapper resource="mybatis/review.xml"/>
    	<mapper resource="mybatis/event.xml"/>
	</mappers>
</configuration>